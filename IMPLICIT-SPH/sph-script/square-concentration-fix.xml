    <Parameter name="Singular Poisson" type="string" value="NotSingular"/>
    <Parameter name="theta" type="double" value="0.5"/>
    <Parameter name="g.x"   type="double" value="0.0"/>
    <Parameter name="g.y"   type="double" value="0.0"/>
    <Parameter name="g.z"   type="double" value="0.0"/>   
  </ParameterList>
  <ParameterList name="Solute Transport">
    <Parameter name="d:0" type="double" value="0.1"/>
    <Parameter name="theta" type="double" value="0.5"/>
    <Parameter name="Use Fixed Particles" type="string" value="Enabled"/>
  </ParameterList>
</ParameterList>

